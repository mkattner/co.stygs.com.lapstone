{
	"name": "WebServiceClient",
	"shortname": "wsc",
	"include": [],
	"defaultWebServiceTimeoutMs": 5000,
	"defaultWebServiceMethod": "post",
	"defaultWebServiceType": "json",
	"useServerShuffle": true,
	"useHeaderToken": false,
	"headerToken": {
		"key": "Authorization",
		"value": "data-html5-themis-token"
	},
	"defaultServer": "template",
	"server": {
		"template": {
			"active": true,
			"pingPath": "",
			"mappings": {
				"post": "application/json",
				"get": "application/json",
				"put": "application/json",
				"delete": "application/json"
			},
			"endpoints": [
				{
					"scheme": "http",
					"scheme_specific_part": "://",
					"host": "localhost",
					"port": 8080,
					"path": "/REST"
				}
			]
		},
		"socket": {
			"active": true,
			"pingPath": "",
			"mappings": {
				"post": "json",
				"get": "query",
				"put": "json",
				"delete": "json"
			},
			"endpoints": [
				{
					"scheme": "ws",
					"scheme_specific_part": "://",
					"host": "localhost",
					"port": 8080,
					"path": "/websocket"
				}
			]
		}
	}
}